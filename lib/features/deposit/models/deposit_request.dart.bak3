/// Deposit Request Model
class DepositRequest {
  final double amount;
  final String currency;
  final String provider;

  const DepositRequest({
    required this.amount,
    required this.currency,
    required this.provider,
  });

  Map<String, dynamic> toJson() => {
        'amount': amount,
        'currency': currency,
        'provider': provider,
      };
}
